
<head>
	<meta name="layout" content="mytheme">
</head>
<div class="tab-content">
  <div class="tab-pane active" id="tab_0">
      <div class="portlet box green">
          <div class="portlet-title">
              <div class="caption">
                  <i class="fa fa-gift"></i>Please enter the document information </div>
              <div class="tools">
                  <a href="javascript:;" class="collapse"> </a>
                  <a href="#portlet-config" data-toggle="modal" class="config"> </a>
                  <a href="javascript:;" class="reload"> </a>
                  <a href="javascript:;" class="remove"> </a>
              </div>
          </div>
          <div class="portlet-body form">
              <!-- BEGIN FORM-->
              <!-- <form action="#" class="form-horizontal">  -->
              <g:uploadForm action="upload" method="post" enctype='multipart/form-data' class="form-horizontal">
                  <div class="form-body">
                      <div class="form-group">
                          <label class="col-md-3 control-label">Customer name:</label>
                          <div class="col-md-4">
                              <input type="text" name="custName" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                      <div class="form-group">
                          <label class="col-md-3 control-label">PAN Number:</label>
                          <div class="col-md-4">
                              <input type="text" name="panNo" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Form Number:</label>
                          <div class="col-md-4">
                              <input type="text" name="formNo" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Owner:</label>
                          <div class="col-md-4">
                              <input type="text" name="ownerName" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Phone number (Work):</label>
                          <div class="col-md-4">
                              <input type="text" name="workPhone" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Phone number (Mobile):</label>
                          <div class="col-md-4">
                              <input type="text" name="mobilePhone" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Registration number:</label>
                          <div class="col-md-4">
                              <input type="text" name="regNo" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Citizenship number:</label>
                          <div class="col-md-4">
                              <input type="text" name="citizenshipNo" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Name of Bank:</label>
                          <div class="col-md-4">
                              <input type="text" name="Bank name:" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Bank branch:</label>
                          <div class="col-md-4">
                              <input type="text" name="bankBranch" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Account name:</label>
                          <div class="col-md-4">
                              <input type="text" name="accountName" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Account number:</label>
                          <div class="col-md-4">
                              <input type="text" name="accountNo" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">Purpose:</label>
                          <div class="col-md-4">
                              <input type="text" name="docPurpose" class="form-control input-circle" placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                       <div class="form-group">
                          <label class="col-md-3 control-label">File upload:</label>
                          <div class="col-md-4">
                              <input type="file" name="file" class="" required placeholder="">
                              <span class="help-block">  </span>
                          </div>
                      </div>
                      
                      
                      
                      
                     
                  </div>
                  <div class="form-actions">
                      <div class="row">
                          <div class="col-md-offset-3 col-md-9">
                              <button type="submit" class="btn btn-circle green">Submit</button>
                              <button type="button" class="btn btn-circle grey-salsa btn-outline">Cancel</button>
                          </div>
                      </div>
                  </div>
              </g:uploadForm>    
              </form>
              <!-- END FORM-->
          </div>
      </div>
                                